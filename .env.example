# Database Configuration
DB_HOST=postgres
DB_PORT=5432
DB_NAME=insurance_db
DB_USER=insurance_user
DB_PASSWORD=change_this_password

# Spring Profiles
SPRING_PROFILES_ACTIVE=dev

# OpenAI Configuration
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_REALTIME_MODEL=gpt-4o-realtime-preview-2024-12-17
OPENAI_TEXT_MODEL=gpt-4o
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Twilio Configuration (for PSTN voice calls)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+1234567890

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-app-password
SMTP_FROM=noreply@insurance-app.com

# SendGrid (Optional alternative to SMTP)
SENDGRID_API_KEY=your_sendgrid_api_key

# JWT Configuration
JWT_SECRET=your-jwt-secret-key-min-256-bits-change-this-in-production
JWT_EXPIRATION_MS=86400000

# File Storage
STORAGE_TYPE=local
STORAGE_LOCAL_PATH=./storage
# For S3-compatible storage
# STORAGE_TYPE=s3
# S3_BUCKET=insurance-documents
# S3_REGION=us-east-1
# S3_ACCESS_KEY=your-access-key
# S3_SECRET_KEY=your-secret-key

# Application Configuration
BACKEND_PORT=8080
FRONTEND_PORT=3000
FRONTEND_URL=http://localhost:3000
BACKEND_URL=http://localhost:8080

# Vector Store Configuration
VECTOR_STORE_TYPE=pgvector
# Optional: Pinecone
# VECTOR_STORE_TYPE=pinecone
# PINECONE_API_KEY=your-pinecone-api-key
# PINECONE_ENVIRONMENT=your-environment
# PINECONE_INDEX=insurance-products

# Mock Mode (for local development without PSTN)
VOICE_MOCK_MODE=true

# Logging
LOG_LEVEL=INFO
